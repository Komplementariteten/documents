   "source": [
    "# Sternschaltung mit Impedanz"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "a8af3a15-feaf-41eb-b58b-bc7d1a6d5489",
   "metadata": {},
   "outputs": [],
   "source": [
    "za = 1 + 1j\n",
    "zb = 1 + 1j\n",
    "zc = 1 + 1j"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "8459ba18-3196-4012-ad2c-bbabac544443",
   "metadata": {},
   "outputs": [],
   "source": [
    "zab = ((za * zb) / zc) + za + zb\n",
    "zac = ((za * zc) / zb) + za + zb\n",
    "zbc = ((zb * zc) / za) + zb + zc"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "51130cd2-1915-4382-b694-508c63e95f59",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "(3+3j)\n",
      "(3+3j)\n",
      "(3+3j)\n"
     ]
    }
   ],
   "source": [
    "print(zab)\n",
    "print(zac)\n",
    "print(zbc)"
   ]
